{
  "name": "@zyi/toolkit-ui",
  "version": "0.0.1",
  "description": "UI Toolkit for zyi typescript monorepo",
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    },
    "./*": {
      "types": [
        "./dist/*.d.ts",
        "./dist/*/index.d.ts"
      ],
      "import": [
        "./dist/*.mjs",
        "./dist/*/index.mjs"
      ],
      "require": [
        "./dist/*.cjs",
        "./dist/*/index.cjs"
      ]
    }
  },
  "scripts": {
    "dev": "pnpm run stub",
    "stub": "unbuild --stub",
    "build": "unbuild"
  },
  "dependencies": {},
  "devDependencies": {}
}
